    <form id="form1" runat="server" defaultbutton="btn_search">
        <div class="container-fluid">

            <!--Suchleiste-->
            <div class="btn-group d-flex justify-content-center" role="group" aria-label="...">

                <!---Sprachauswahl-->
                <dot:ListBox Size="0" DataSource="{value: Languages}" SelectedValue="{value: SelectedLanguage}" class="dropdown btn btn-secondary bg-primary flex-grow-1" />

            </div>
            <div class="input-group">
                <dot:Button ID="btn_keyboard" runat="server" class="btn btn-primary" Text="⌨" Click="{command: btn_keyboard_Click()}" />
                <dot:TextBox ID="txt_search" class="form-control" text="{value: txt_search}" placeholder="Gib ein deutsches Wort ein"></dot:TextBox>
                <dot:Button ID="btn_search" runat="server" class="btn btn-secondary bg-primary" Click="{command: Search()}" Text="Suche" IsSubmitButton="true" />
                <button type="button" class="btn btn-outline-secondary dropdown-toggle dropdown-toggle-split" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    <span class="sr-only"></span>
                </button>
                <div class="dropdown-menu">
                    <dot:Button ID="btn_like" runat="server" class="dropdown-item" Click="{command: SearchLike()}" Text="Unscharfe Suche" />
                    <dot:Button ID="btn_exact" runat="server" class="dropdown-item" Click="{command: SearchExact()}" Text="Exakte Suche" />
                </div>
            </div>
